{
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 4,
		"printWidth": 120,
		"useTabs": true,
		"semi": false,
		"singleQuote": true
	},
	"private": true,
	"scripts": {
		"dev": "npm run development",
		"development": "mix",
		"watch": "mix watch",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"prod": "npm run production",
		"production": "mix --production"
	},
	"devDependencies": {
		"axios": "^0.21",
		"laravel-mix": "^6.0.6",
		"lodash": "^4.17.19",
		"postcss": "^8.1.14"
	}
}
